/** @param {import(".").NS } ns */
export async function main(ns) {
    let N = 4;
    let dp = Array.from({ length: N + 1 }, (_, i) => 0);

    dp[0] = 1;

    for (let row = 1; row < N + 1; row++) {
        for (let col = 1; col < N + 1; col++) {
			ns.tprintf("[%s]", dp)
			ns.tprintf("  => [%d:%d] dp[col]: %d", dp[col])
            if (col >= row) {
                dp[col] = dp[col] + dp[col - row];
			}
        }
    }

    ns.tprintf("Ways: %d => %d", N, dp[N]);
}
